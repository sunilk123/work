<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sunil.ParkingLotEntity" table="Parking_Lot" schema="dbo" catalog="ParkingService">
        <id name="parkinglotid">
            <column name="Parkinglotid" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="nchar(30)" length="30"/>
        </property>
        <property name="size">
            <column name="size" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>