<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sunil.AdminLoginEntity" table="Admin_Login" schema="dbo" catalog="ParkingService">
        <id name="adminid">
            <column name="adminid" sql-type="int"/>
        </id>
        <property name="username">
            <column name="username" sql-type="nchar(30)" length="30"/>
        </property>
        <property name="password">
            <column name="password" sql-type="nchar(30)" length="30"/>
        </property>
    </class>
</hibernate-mapping>